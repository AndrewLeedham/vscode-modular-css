{
    "name": "vscode-modular-css",
    "displayName": "Modular-CSS Support",
    "description": "An extension to add Modular-CSS syntax highlighting to VSCode.",
    "version": "1.0.0",
    "publisher": "andrewleedham",
    "author": {
        "name": "Andrew Leedham",
        "url": "http://andrewleedham.me"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "modular-css",
            "aliases": ["CSS (modular-css)", "modular-css"],
            "extensions": [".css"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "modular-css",
            "scopeName": "source.css",
            "path": "./syntaxes/css.tmLanguage"
        }]
    },
    "repository": "https://github.com/AndrewLeedham/vscode-modular-css",
    "keywords": [
        "language",
        "language-support",
        "vscode",
        "extension",
        "css",
        "modular-css"
    ],
    "icon": "./icon.png",
    "galleryBanner":{
        "color": "#580065"
    }
}