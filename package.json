{
    "name": "vscode-modular-css",
    "displayName": "Modular-CSS Support",
    "description": "An extension to add Modular-CSS syntax highlighting to VSCode.",
    "version": "1.2.1",
    "publisher": "andrewleedham",
    "scripts": {
        "push": "vsce publish"
    },
    "author": {
        "name": "Andrew Leedham",
        "url": "http://andrewleedham.me"
    },
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "scopeName": "source.modular.css",
                "path": "./syntaxes/modular-css.json",
                "injectTo": ["source.css"]
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AndrewLeedham/vscode-modular-css"
    },
    "keywords": [
        "language support",
        "vscode",
        "extension",
        "css",
        "modular-css"
    ],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#580065"
    },
    "devDependencies": {
        "vso-node-api": "^6.1.2-preview"
    },
    "dependencies": {
        "vsce": "^1.36.1"
    }
}
